git config --global user.name "username"
git config user.name "username"
git config --global user.email [your email address here]
git config user.email [your email address here]

git config --list

echo "# xxxxx" >> README.md
git init
git add README.md
git commit -m "first commit"
git remote add origin https://github.com/sunvodz/xxxxx.git
git push -u origin master

git pull

git branch sunvo

git checkout sunvo             create branch name dev

git add *                    add all file go to in branch dev 

git status                   checking status add file

git commit -m "file commit"  commit branch dev and branch master 

git checkout master          back to branch master


git push -u origin master    up file in commit go to git 



gitk --all 

git reset soft/mixed/hard idcommit 
git reset HEAD~2
git reset --soft HEAD

git pull --rebase 
git fetch origin
git pull [your branch] 
